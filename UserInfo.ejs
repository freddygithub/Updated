<body class="bg-gradient-primary">

<div class="container my-5">

<nav class="navbar navbar-expand-lg navbar-dark fixed-top border bg-light" id="mainNav">
    <div class="container">
      <div class="container" style="width: 250px; margin: 0;">
          <div class="row">
            <div class="col" ><div class="echelonLogo-2"></div></div>
            <div class="w-100"></div>
            <div class="col" ><div class="logoy-2"></div></div>
          </div>
        </div>
      <a class="signinText" href="login.html">Sign In</a>
    </div>
</nav>
      
    <div class="row justify-content-center">

      <div class="col-lg-8 col-lg-12 col-md-9">
        <div class="card o-hidden border-0 my-5">
          <div class="card-body p-0">
            <!-- Nested Row within Card Body -->
            <div class="row justify-content-center">
              <div class="col-lg-6">
                <div class="p-5">
                  <div class="text-center">
                      <div class="cir"><i class="fas fa-check fa-2x bigCheck"></i></div>
                      <br>
                    <h1 class="h4 mb-4 signinHeader" style="white-space: nowrap;">Creating your account.</h1>
                      <p>We need your information to accurately calculate the number of calories burned and heart rate</p>
                  </div>
                  <form class="user" method="get" action="payment">
                      <input id="plan" name="plan" hidden>
                      <input type="hidden" value="<%= uinfo %>" name="uinfo">
                      <div  class="text-center m-auto">
                        <div class="row col">
                          <label class="contextBody">Gender</label>
                        </div>
                        <div class="row mb-2">
                            <div class="col">
                                <div class="form-group w-100">
                                    <select class="form-control infoGender-2" name="gender" id="gender">
                                      <option>Female</option>
                                      <option>Male</option>
                                      <option selected>Other</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                          
                        <div class="row text-left">
                            <div class="col">
                              <label class="contextBody">Weight (pounds)</label>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col">
                                <input class="form-control infoGender-2" type="number" name="weight" id="weight" min="0" data-bind="value:replyNumber" />
                            </div>
                        </div>
                          <br>
                        <div class="row text-left">
                            <div class="col">
                              <label class="contextBody">Height</label>
                            </div>
                        </div>
                        <div class="row mb-4">
                            <div class="col-md-4">
                                <input class="form-control birthday-sm" type="number" step="0.01" name="height" id="height" min="0" data-bind="value:replyNumber" />
                            </div>
                            <div class="col-md-4">
                                <input class="form-control birthday-sm" type="number" step="0.01" name="height" id="inches" min="0" data-bind="value:replyNumber" />
                                
                            </div>
                            <div class="col-md-4">
                                <select class="form-control birthday-sm" name="height" id="heightSelect">
                                      <option id="centy">Centimeters</option>
                                      <option selected>feet & inches</option>
                                </select>
                            </div>
                        </div>
                       
                          <div class="row col">
                            <label class="contextBody">Birthday</label>
                        </div>
                        <div class="row mb-4">
                            <div class="col-md-4">
                                <select class="form-control birthday-sm" id="birthM">
                                  <option selected>January</option>
                                    <option>February</option>
                                    <option>March</option>
                                    <option>April</option>
                                    <option>May</option>
                                    <option>June</option>
                                    <option>July</option>
                                    <option>August</option>
                                    <option>September</option>
                                    <option>October</option>
                                    <option>November</option>
                                    <option>December</option>
                                </select>
                            </div>
                            <div class="col-md-4">
                                <select class="form-control birthday-sm"id="birthD">
                                    <option selected>1</option>
                                    <option>2</option>
                                    <option>3</option>
                                    <option>4</option>
                                    <option>5</option>
                                    <option>6</option>
                                    <option>7</option>
                                    <option>8</option>
                                    <option>9</option>
                                    <option>10</option>
                                    <option>11</option>
                                    <option>12</option>
                                    <option>13</option>
                                    <option>14</option>
                                    <option>15</option>
                                    <option>16</option>
                                    <option>17</option>
                                    <option>18</option>
                                    <option>19</option>
                                    <option>20</option>
                                    <option>21</option>
                                    <option>22</option>
                                    <option>23</option>
                                    <option>24</option>
                                    <option>25</option>
                                    <option>26</option>
                                    <option>27</option>
                                    <option>28</option>
                                    <option>29</option>
                                    <option>30</option>
                                    <option>31</option>
                                </select>
                            </div>
                            <div class="col-md-4">
                                <select class="form-control birthday-sm" id="birthY">
                                </select>
                            </div>
                        </div>
                          
                    <button type="submit" id="sumbitBtn" class="btn btn-danger btn-user btn-block signupText">
                      Continue
                    </button>
                    </div>
                  </form>
                </div>
              </div>
            </div>
          </div>
        </div>

      </div>

    </div>

  </div>
    
