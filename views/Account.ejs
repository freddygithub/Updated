    <div class="row justify-content-center">

      <div class="col-lg-8 col-lg-12 col-md-9">
        <div class="card o-hidden border-0 my-5">
          <div class="card-body p-0">
            <!-- Nested Row within Card Body -->
            <div class="row justify-content-center">
              <div class="col-lg-6">
                <div class="p-5">
                  <div class="text-center">
                      <div class="cir"><i class="fas fa-check fa-2x bigCheck"></i></div>
                      <br>
                      <div class="stepsText">Step <b>2</b> of <b>3</b></div>
                    <h1 class="h4 mb-4 signinHeader" style="white-space: nowrap;">Create your account.</h1>
                      <p>Just a few more steps and you're done!</p>
                  </div>
                  <form class="user" method="get" action="userinfo">
                      <input id="plan" name="plan" hidden>
                      <div class="text-center">
                        <div class="form-group">
                          <input type="email" name="email" class="form-control form-control-user" id="exampleInputEmail" aria-describedby="emailHelp" placeholder="Enter Email Address...">
                        </div>
                          <br>
                        <div class="form-group">
                          <input type="password" class="form-control form-control-user" id="psw" name="psw" placeholder="Password" pattern="(?=.*\d)(?=.*[a-z])(?=.*[A-Z]).{8,}" title="Must contain at least one number and one uppercase and lowercase letter, and at least 8 or more characters">
                        </div>
                        <div class="form-group">
                          <input type="password" class="form-control form-control-user" placeholder="Verify Password">
                        </div>
                          
                        <div id="message">
                          <h5>Password must contain the following:</h5>
                          <p id="letter" class="invalid">A <b>lowercase</b> letter</p>
                          <p id="capital" class="invalid">A <b>capital (uppercase)</b> letter</p>
                          <p id="number" class="invalid">A <b>number</b></p>
                          <p id="length" class="invalid">Minimum <b>8 characters</b></p>
                        </div>
                          
                    </div>
                      <br>
                    <button type="submit" id="sumbitBtn" class="btn btn-danger btn-user btn-block signupText">
                      Continue
                    </button>
                  </form>
                </div>
              </div>
            </div>
