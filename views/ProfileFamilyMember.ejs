<!-- line modal for canceling -->
<div class="modal fade" id="squarespaceModal1" role="dialog" aria-labelledby="modalLabel">
  <div class="modal-dialog">
    <div class="modal-content">
        <div class="modal-header">
            <h3 class="modal-title" id="lineModalLabel">Access Denied</h3>
            <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
        </div>
        <div class="modal-body">
            <label class="modal-label subText">You don't have permission to perform this action</label>
        </div>
        <div class="modal-footer">
            <div class="btn-group btn-group-justified" role="group" aria-label="group button">
                <div class="btn-group btn-delete hidden" role="group">
                    <a class="btn btn-danger btn-user btn-block signupText" data-dismiss="modal">
                      exit
                    </a>
                </div>
            </div>
        </div>
    </div>
  </div>
</div>
  
<!-- line modal for phone number -->
<div class="modal fade" id="squarespaceModal3" role="dialog" aria-labelledby="modalLabel">
  <div class="modal-dialog">
    <div class="modal-content">
        <div class="modal-header">
            <h3 class="modal-title" id="lineModalLabel">Change Phone Number</h3>
            <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
        </div>
        <div class="modal-body">
            <label class="modal-label">Mobile Phone Number</label>
